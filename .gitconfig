[log]
  date = relative
[format]
  pretty = format:%C(yellow)%h %Cblue%>(12)%ad %Cgreen%<(7)%aN%Cred%d %Creset%s
[alias]
  b = branch
  branch-name = "!git rev-parse -abbrev-ref HEAD"
  ca = commit --amend
	co = checkout
  d = diff
  l = log
  publish = "!git push -u origin $(git branch-name)"
  s = status
[core]
	editor = vim
[push]
	default = simple
